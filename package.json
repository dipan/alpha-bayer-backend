{
  "name": "y",
  "version": "1.0.0",
  "description": "note-typescript-boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:dev": "tsc -p tsconfig.build.json && cp .env.dev ./dist/.env && cp package.json ./dist",
    "build:local": "rm -rf /dist && dotenv --dotenv .env.local tsc -p tsconfig.build.json",
    "build:prod": "rm -rf /dist && dotenv --dotenv .env.prod tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "compile": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "test": "jest",
    "server:install": "npm install --omit=dev",
    "start": "node dist/src/app.js"
  },
  "author": "dipan.dev",
  "license": "ISC",
  "dependencies": {
    "@xata.io/client": "^0.28.3",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.0.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cronstrue": "^2.27.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^7.1.1",
    "mysql2": "^3.3.3",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.8",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.16",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.1",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.1.3",
    "@types/node-cron": "^3.0.7",
    "@types/nodemailer": "^6.4.14",
    "@types/sequelize": "^4.28.15",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/winston": "^2.4.4",
    "dotenv-cli": "^7.2.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
